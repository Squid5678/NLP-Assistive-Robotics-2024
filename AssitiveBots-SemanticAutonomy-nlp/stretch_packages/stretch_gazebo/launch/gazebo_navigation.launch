<launch>

  <arg name="debug_directory" value="$(env HELLO_FLEET_PATH)/debug/"/>

  <!-- Gazebo Navigation Manager -->
  <node name="gazebo_navigation" pkg="stretch_gazebo" type="gazebo_navigation" output="screen">
